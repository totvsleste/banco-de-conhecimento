Function LoadFils()
Local aFiliais	:= {}
Local aSM0		:= AdmAbreSM0()
Local nX		:= 0

For nX := 1 to Len(aSM0)
    If Alltrim(aSM0[nX][1]) != Alltrim(cEmpAnt)
    	Loop
    Else
    	aadd(aFiliais,aSM0[nX][2])
    Endif
    
Next nX


Return aFiliais

